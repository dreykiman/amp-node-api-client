{
  "name": "amp-node-api-client",
  "version": "1.0.0",
  "description": "API for AMP Bot",
  "main": "./dist/app.js",
  "scripts": {
    "build": "babel src --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/app.js",
    "www": "npm run build && forever start dist/www.js",
    "test": "npm run build && forever start --minUptime 6000000 --spinSleepTime 300000 dist/app.js"
  },
  "author": "Andrey",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.3",
    "binance-api-node": "^0.8.13",
    "ethers": "^4.0.7",
    "express": "^4.16.4",
    "forever": "^0.15.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "ws": "^6.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  }
}
